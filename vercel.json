{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_GITHUB_CLIENT_ID": "@github_client_id",
    "REACT_APP_GITHUB_CLIENT_SECRET": "@github_client_secret",
    "REACT_APP_FRONTEND_URL": "@frontend_url",
    "REACT_APP_GITHUB_API_URL": "https://api.github.com",
    "REACT_APP_GITHUB_PAGES": "false",
    "REACT_APP_DISABLE_OAUTH": "false"
  }
}
